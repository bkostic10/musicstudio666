<html t:type="layout" title="Add Studio"
      xmlns:t="http://tapestry.apache.org/schema/tapestry_5_1_0.xsd"
      xmlns:p="tapestry:parameter">
<head></head><body>
    Add Studio:
    <br/><br/>
    <t:form t:id="addStudioForm">
    <t:errors/>
        Studio ID:<br/>
        <div><t:textfield t:value="studioID"
        t:validate="required"/></div>

        Studio Name:<br/>
        <div><t:textfield t:value="studioName"
        t:validate="required"/></div>

        Studio Address:<br/>
        <div><t:textfield t:value="studioAddress"
        t:validate="required"/></div>

        <br/>
        <input type="submit" value="Submit"/>
    </t:form>
    <br/>
    <div>${name}</div><br/>
    
    <t:pagelink page="Studios">Back</t:pagelink>
</body>
</html>
