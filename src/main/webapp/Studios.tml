<html t:type="layout" title="Studios"
      xmlns:t="http://tapestry.apache.org/schema/tapestry_5_1_0.xsd"
      xmlns:p="tapestry:parameter">
<head></head><body>
    <p>Choose a Studio:</p>
    
    
        
    <div t:type="loop" t:source="allStudios" t:value="studio">
    
        <t:ActionLink t:context="studio.studioName"
        t:id="studioDetailsLink">${studio.studioName}</t:ActionLink>
    
    </div>
    
    <br/>
    <t:PageLink t:page="Index">Back To The Start Page</t:PageLink>
</body>
<p:sidebar><br/>
        <center><h1>Welcome ${userWelcome}</h1></center>
<br/><br/>
    </p:sidebar>
</html>
