<html t:type="layout" title="Rooms"
      xmlns:t="http://tapestry.apache.org/schema/tapestry_5_1_0.xsd"
      xmlns:p="tapestry:parameter">
<head></head><body>
   <p>Choose a Room:</p>
    <p></p>
    <div t:type="loop" t:source="studio.getAllRooms()" t:value="room">
        <t:ActionLink t:context="room.roomId"
        t:id="roomDetailsLink">${room.roomName}</t:ActionLink>
    </div><br/><div>${name}</div>

    <br/>
    <t:PageLink t:page="StudioDetails">Back To The Studio Details</t:PageLink>
</body>
<p:sidebar><br/>
        <h1>Welcome:</h1>
        <center><user>${user.firstName} ${user.lastName}</user></center>
<br/><br/>
    </p:sidebar>
</html>
